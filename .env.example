# Contentful CMS Credentials
# Copy this file to .env and fill in your actual values

# Required: Your Contentful Space ID
CONTENTFUL_SPACE_ID=your_space_id_here

# Required: Contentful Delivery API Token (for production builds)
CONTENTFUL_ACCESS_TOKEN=your_delivery_api_token_here

# Optional: Contentful Preview API Token (for draft content preview)
CONTENTFUL_PREVIEW_TOKEN=your_preview_api_token_here

# Optional: Build mode (production | preview)
CONTENTFUL_MODE=production
